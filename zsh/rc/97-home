# Final home hacks
if [ `cat $HOME/.dotfile-environment` = 'home' ]; then
    alias ecr-login='eval $(aws ecr get-login --no-include-email --region us-east-2)'

    export SAFEPLACE=yep

    alias kq="kubectl --context sterling-cluster3.qubling.cloud"
    alias k4="kubectl --context main-cluster"
fi

