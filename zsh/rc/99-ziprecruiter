# Final work hacks
if [ `cat $HOME/.dotfile-environment` = 'ziprecruiter' ]; then
  export STARTERVIEW=$HOME/ziprecruiter

  export GOPATH=$HOME/go
  export PATH=$PATH:$HOME/go/bin

  export PATH=$STARTERVIEW/bin:$STARTERVIEW/infrastructure/terraform/bin:$STARTERVIEW/infrastructure/dana:$PATH
fi

